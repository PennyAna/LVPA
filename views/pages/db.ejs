<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>
<body>
<% include ../partials/nav.ejs %>
<div class="container">
<h2>Database Results</h2>
<ul>
  Hello there!
    <% results.forEach(function(results) { %>
        <li>
            <%= results.title_id %> - 
            <%= results.title_name %> -
            <%= results.genre_id %> - 
            <%= results.media_type_tv %> - 
            <%= results.media_type_film %> - 
            <%= results.media_type_other %>
          </li>
    <% }); %>
    <% results.forEach(function(results_genre)) { %>
      <li>
        <% results_genre.genre_id %> - 
        <%= results_genre.genre_name %>
      </li>
    <% }); %>
    <% results.forEach(function(results_user)) { %>
      <li>
        <% results_user.username %> - 
        <% results_user.password %>
      </li>
    <% }); %>
</ul>
<ul>
  Hello there!
    <% results_genre.forEach(function(r) { %>
        <li>
            <%= r.genre_id %> - 
            <%= r.genre_name %> 
          </li>
    <% }); %>
</ul>
<ul>
  Hello there!
    <% results_user.forEach(function(r) { %>
        <li>
            <%= r.username %> - 
            <%= r.password %> 
          </li>
    <% }); %>
</ul>
</div>
</body>
</html>
